<div class="row">
  <div class="col-sm-12">
        <h1 class="main-text">{{title}}</h1>
        <main>
            <div id="error" class="error" hidden>I'm an error</div>
            <div class="form-container">
                <form id="myForm">
                    <div class="form-group">
                        <label id="formLabel" for="inputNumber">Search Number</label>
                        <input type="number" class="form-control" id="inputNumber" name="inputNumber" placeholder="Search">
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-default">Submit</button>
                    </div>
                </form>
            </div>
            <ul id="results"></ul>
        </main>
  </div>
</div>